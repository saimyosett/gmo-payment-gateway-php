{
    "name": "saimyosett/gmo-payment-gateway-php",
    "description": "GMO Payment Gateway PHP Library",
    "keywords": [
        "GMO",
        "Payment",
        "Gateway"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sai Myo Sett",
            "email": "saimyosett22@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.5",
        "nesbot/carbon": "^2.71",
        "phpseclib/phpseclib": "^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "pestphp/pest": "^2.19"
    },
    "autoload": {
        "psr-4": {
            "GmoPaymentGateway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GmoPaymentGateway\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "scripts": {
        "test": [
            "./vendor/bin/pest"
        ]
    }
}
